@using System.Configuration
@{
    ViewBag.Title = "Example Dynamic Configuration";
}

<p>
    Current Example app setting values are:
    <ul>
        @foreach (var setting in ConfigurationManager.AppSettings.AllKeys)
        {
            <li><label>@setting</label> <span>@ConfigurationManager.AppSettings[setting]</span></li>
        }
    </ul>
</p>
